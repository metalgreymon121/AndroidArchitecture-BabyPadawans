<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.architecture.babypadawans"
    >

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme"
      >
    <activity
        android:name="com.architecture.babypadawans.views.login.LoginActivity"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.NoActionBar"
        >
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity
        android:name="com.architecture.babypadawans.views.register.RegisterActivity"
        android:theme="@style/AppTheme.NoActionBar"
        />


    <activity
        android:name="com.architecture.babypadawans.views.songs.show.SongActivity"
        />

    <activity
        android:name="com.architecture.babypadawans.views.songs.SongsActivity"
        android:label="@string/title_activity_songs"
        />
  </application>

</manifest>
